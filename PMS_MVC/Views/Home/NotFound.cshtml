@{
    Layout = "_NullLayout";
    ViewData["Title"] = "404 - Invalid Request";
}
<div class="text-center p-5 shadow-lg rounded bg-white">
    <h1 class="display-1 text-danger"><i class="fas fa-exclamation-triangle text-warning"></i> 404</h1>
    <h2 class="text-muted mt-3">Oops! Something went wrong or the requested URL was not found.</h2>
    <p class="text-muted">The page you are looking for might have been removed or unavailable.</p>
    @* <a href="/" class="btn btn-primary mt-4"><i class="fas fa-home"></i> Go to Homepage</a> *@
    <a href="javascript:history.back()" class="btn btn-secondary mt-4 ml-2"><i class="fas fa-arrow-left"></i> Go Back</a>
</div>